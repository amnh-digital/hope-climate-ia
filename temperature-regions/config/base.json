{
  "graphics": {
    "el": "#graphics",
    "time": 0.5,
    "zone": 0.5,
    "margin": [0.12, 0.06, 0.07, 0.05],
    "plotBGColor": "$0xprimaryDarkest",
    "plotLineColor": "$0xblack",
    "marker": {
      "color": "$0xhighlight",
      "textStyle": {
        "fill": "$#highlight",
        "fontSize": 0.05,
        "fontWeight": "bold",
        "fontFamily": "Balance"
      }
    },
    "xAxis": {
      "height": 0.08,
      "tickColor": "$0xprimaryDark",
      "textStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.05,
        "fontFamily": "Balance"
      },
      "subtextStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.04,
        "fontFamily": "Balance"
      }
    },
    "yAxis": {
      "width": 0.125,
      "textStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.05,
        "fontFamily": "Balance"
      },
      "subtextStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.04,
        "fontFamily": "Balance"
      }
    }
  },
  "map": {
    "dir": "img/frames/",
    "el": "#map",
    "imgEl": "#temperature-map",
    "helperEl": "#map-helper",
    "clipImgEl": "#worldmap",
    "yearEl": ".map-year",
    "latEl": ".map-latitude"
  },
  "messages": {
    "el": "#messages"
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 10,
    "sleepAfter": 60
  }
}
