{
  "graphics": {
    "el": "#graphics",
    "time": 0.5,
    "zone": 0.5,
    "margin": [0.12, 0.06, 0.07, 0.05],
    "marker": {
      "textStyle": {
        "fill": "#e2bb3d",
        "fontSize": 0.05,
        "fontWeight": "bold"
      }
    },
    "xAxis": {
      "height": 0.08,
      "textStyle": {
        "fill": "#505454",
        "fontSize": 0.05
      },
      "subtextStyle": {
        "fill": "#505454",
        "fontSize": 0.04
      }
    },
    "yAxis": {
      "width": 0.125,
      "textStyle": {
        "fill": "#505454",
        "fontSize": 0.05
      },
      "subtextStyle": {
        "fill": "#505454",
        "fontSize": 0.04
      }
    }
  },
  "map": {
    "dir": "img/frames/",
    "el": "#map",
    "imgEl": "#temperature-map",
    "helperEl": "#map-helper",
    "clipImgEl": "#worldmap",
    "yearEl": ".map-year"
  },
  "messages": {
    "el": "#messages"
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 5,
    "sleepAfter": 30
  }
}
