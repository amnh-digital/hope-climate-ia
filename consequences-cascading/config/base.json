{
  "network": {
    "el": "#network",
    "angleThreshold": 60,
    "transitionMs": 1000,
    "resetAfterMs": 2000,
    "nodeRadiusRange": [0.02, 0.08],
    "nodeAlphaRange": [0.3, 1.0],
    "nodeLineWidthRange": [3, 8],
    "nodeDashGapRange": [5, 0],
    "nodeDashWidthRange": [5, 5],
    "nodeDashMs": 500,
    "nodeMs": 1500,
    "nodeTransitionMs": 1000,
    "nodeContentDelayMs": 500,
    "nodeContentMs": 1000,
    "nodeContentWidth": 0.2,
    "nodeContentHeight": 0.12,
    "nodeImageWidth": 0.2,
    "nodeLabelTextStyle": {
      "fontSize": 0.02,
      "fill": "#999999",
      "fontFamily": "Balance"
    },
    "nodeBodyTextStyle": {
      "fontSize": 0.015,
      "fill": "#999999",
      "fontFamily": "Balance",
      "wordWrap": true,
      "lineHeight": 1.5
    },
    "rootNode": {
      "radius": 0.1,
      "x": 0.5,
      "y": 0.15,
      "color": "0xd64724",
      "textStyle": {
        "fontSize": 0.033,
        "fill": "#ffffff",
        "fontFamily": "Balance"
      }
    },
    "soundSprites": ["low", "medium", "high", "higher"]
  },
  "sound": {
    "enable": 1,
    "audio": ["../shared/audio/pop.mp3"],
    "volume": 0.4,
    "limitLastPlayedMs": 50,
    "limitSameLastPlayedMs": 200,
    "sprites": {
      "higher": [1350, 1000],
      "high": [0, 1000],
      "medium": [4000, 1000],
      "low": [5200, 1000]
    }
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 15,
    "sleepAfter": 120
  }
}
