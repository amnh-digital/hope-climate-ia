{
  "slideshow": {
    "el": "#slideshow",
    "captionsEl": "#captions",
    "width": 1366,
    "height": 768,
    "animationMs": 1000
  },
  "globe": {
    "el": "#globe",
    "borderColor": "$#primaryDarker",
    "oceanColor": "$#primaryDark",
    "highlightColor": "$#highlight",
    "animationMs": 1000
  },
  "controls": {
    "gamepadMappings": {
      "horizontal": 1
    },
    "keyboardMappings": {
      " ": "next",
      "1": "next"
    }
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 5,
    "sleepAfter": 60
  }
}
