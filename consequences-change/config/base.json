{
  "slideshow": {
    "el": "#slideshow",
    "captionsEl": "#captions",
    "width": 1366,
    "height": 768
  },
  "globe": {
    "el": "#globe",
    "landColor": "#595b5e",
    "borderColor": "#cccccc",
    "oceanColor": "#1e1f21",
    "highlightColor": "#ffe100"
  },
  "controls": {
    "gamepadMappings": {
      "horizontal": 1
    },
    "keyboardMappings": {
      " ": "next",
      "1": "next"
    }
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 15,
    "sleepAfter": 120
  }
}
