{
  "animationMs": 1000,
  "slideshow": {
    "el": "#slideshow",
    "captionsEl": "#captions",
    "width": 1366,
    "height": 768
  },
  "globe": {
    "el": "#globe",
    "borderColor": "$#primaryDarker",
    "oceanColor": "$#primaryDark",
    "highlightColor": "$#highlight"
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 10,
    "sleepAfter": 60
  }
}
