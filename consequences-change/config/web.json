{
  "controls": {
    "uiMappings": {
      "horizontal": {
        "label": "Change time",
        "el": "slider-horizontal",
        "options": {
          "min": 0,
          "max": 1,
          "step": 0.001,
          "value": 0.5
        }
      }
    },
    "keyboardMappings": {
      " ": "next",
      "1": "next"
    },
    "buttonMappings": {
      "next": {
        "el": "button-next",
        "text": "next"
      }
    }
  }
}
