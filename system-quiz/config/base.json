{
  "quiz": {
    "el": "#quiz",
    "answerWaitMs": 8000
  },
  "sound": {
    "enable": 0,
    "audio": ["../shared/audio/quiz.mp3"],
    "volume": 1.0,
    "limitLastPlayedMs": 50,
    "limitSameLastPlayedMs": 200,
    "sprites": {
      "correct": [0, 1400],
      "incorrect": [1400, 3000]
    }
  },
  "check": {
    "enable": 1,
    "checkInterval": 5,
    "sleepAfter": 30,
    "custom": {
      "prompt": "check"
    }
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 10,
    "sleepAfter": 60,
    "custom": {
      "prompt": "start"
    }
  }
}
