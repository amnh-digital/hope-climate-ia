{
  "graphics": {
    "el": "#graphics",
    "margin": [0.1, 0.15, 0.1, 0.1],
    "sleepTransitionMs": 2000,
    "plotBGColor": "$0xprimaryDarkest",
    "plotLineColor": "$0xblack",
    "plotTickColor": "$0xprimaryDark",
    "observedColor": "$0xprimaryDarker",
    "restingLineColor": "$0xsecondary",
    "xAxis": {
      "height": 0.0667,
      "labelEvery": 20,
      "tickEvery": 5,
      "textStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.03,
        "fontFamily": "Balance"
      },
      "subtextStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.0275,
        "fontFamily": "Balance"
      }
    },
    "yAxis": {
      "width": 0.0667,
      "labelCount": 6,
      "step": 0.1,
      "minBounds": [-0.2, 0.2],
      "tickEvery": 0.25,
      "textStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.03,
        "fontFamily": "Balance"
      },
      "subtextStyle": {
        "fill": "$#primaryDark",
        "fontSize": 0.03,
        "fontFamily": "Balance"
      },
      "hotColor": "$0xwarm",
      "coolColor": "$0xcool"
    },
    "transitionStep": 0.01,
    "cord": {
      "curveRatio": 0.45,
      "ampMin": 0.05,
      "oscRange": [0.005, 0.01],
      "tensityRange": [0.04, 0.08],
      "ampRange": [10, 50]
    }
  },
  "sound": {
    "enable": 1,
    "audio": ["../shared/audio/orchestral_harp.mp3"],
    "volume": 1.0,
    "limitLastPlayedMs": 50,
    "limitSameLastPlayedMs": 200,
    "sprites": {
      "Db3": [60725, 2429],
      "E3": [70441, 2429],
      "Gb3": [109305, 2429],
      "Ab3": [12145, 2429],
      "A3": [2429, 2429],
      "B3": [21861, 2429],
      "Db4": [63154, 2429],
      "E4": [72870, 2429],
      "Gb4": [111734, 2429],
      "Ab4": [14574, 2429],
      "A4": [4858, 2429],
      "B4": [24290, 2429],
      "Db5": [65583, 2429]
    }
  },
  "messages": {
    "el": "#messages"
  },
  "sleep": {
    "enable": 1,
    "checkInterval": 10,
    "sleepAfter": 60
  }
}
